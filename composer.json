{
    "name": "laravie/api",
    "description": "A RESTful API package for the Laravel and Lumen frameworks.",
    "keywords": ["api", "dingo", "laravel", "restful"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jason Lewis",
            "email": "jason.lewis1991@gmail.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dingo\\Api\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dingo\\Api\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/routing": " ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "laravie/blueprint": "1.x-dev"
    },
    "require-dev": {
        "laravel/lumen-framework": "^8.0",
        "league/fractal": "^0.19.0",
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^5.0 | ^6.0",
        "tymon/jwt-auth": "^1.0.x-dev"
    },
    "replace": {
        "dingo/api": "*"
    },
    "suggest": {
        "league/fractal": "Transform response using Fractal (^0.19.0).",
        "tymon/jwt-auth": "Protect your API with JSON Web Tokens (^1.0.x-dev)."
    },
    "extra": {
        "branch": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
